<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ChristianJodi.Views.ProfileDetailsPage"
             xmlns:custom="clr-namespace:ChristianJodi.FontsAwesome;assembly=ChristianJodi"
             xmlns:FoA="clr-namespace:ChristianJodi.FontsAwesome;assembly=ChristianJodi"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             xmlns:carousel="clr-namespace:Syncfusion.Maui.Carousel;assembly=Syncfusion.Maui.Carousel"
            xmlns:syncfusion="clr-namespace:Syncfusion.Maui.Accordion;assembly=Syncfusion.Maui.Expander"
            xmlns:thickness ="clr-namespace:Microsoft.Maui;assembly=Microsoft.Maui"
            Background="White"
             Title="Profile Details">

    <ContentPage.Resources>
        <ResourceDictionary>
            <toolkit:BoolToObjectConverter x:Key="BoolToObjectConverter-Liked" TrueObject="#008000" FalseObject="#808080" />
            <toolkit:BoolToObjectConverter x:Key="BoolToObjectConverter-Blocked" TrueObject="#FF6347" FalseObject="#808080" />
            <DataTemplate x:Key="itemTemplate">
                <Image Source="{Binding Image}" 
                       Aspect="AspectFit"/>
            </DataTemplate>
        </ResourceDictionary>
    </ContentPage.Resources>

    <ScrollView>
        <ContentView>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition>
                    </ColumnDefinition>
                </Grid.ColumnDefinitions>

                <carousel:SfCarousel
                            x:Name="carousel"
                            ItemTemplate="{StaticResource itemTemplate}"
                            HeightRequest="400"
                            ItemHeight="300"
                            ItemWidth="200"
                            ItemsSource="{Binding ProfilePhotos}"
                            ViewMode="Default"
                            WidthRequest="800" />
                 

                <ContentView Grid.Row="1" Grid.ColumnSpan ="2">
                    <syncfusion:SfAccordion x:Name="accordion" ExpandMode="SingleOrNone"
                        VerticalOptions="{OnPlatform Default=Fill}" 
                        IsClippedToBounds="True"
                        Grid.ColumnSpan="{OnPlatform Default=2, MacCatalyst=1,WinUI=1}" 
                        Grid.Column="{OnPlatform Default=0, MacCatalyst=1,WinUI=1}" 
                        HorizontalOptions="Fill">

