                                    <Label Grid.Row="6" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Brothers Married" />

                                    <Label Grid.Row="6" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  BrothersMarried}" />

                                    <Label Grid.Row="7" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "About Brothers" />

                                    <Label Grid.Row="7" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  AboutBrothers}" />

                                    <Label Grid.Row="8" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Sisters" />

                                    <Label Grid.Row="8" Grid.Column="1"
        Margin="0,0,0,5"
