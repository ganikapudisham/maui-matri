<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ChristianJodi.Views.ProfileDetailsPage"
             xmlns:custom="clr-namespace:ChristianJodi.FontsAwesome;assembly=ChristianJodi"
             xmlns:FoA="clr-namespace:ChristianJodi.FontsAwesome;assembly=ChristianJodi"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             xmlns:carousel="clr-namespace:Syncfusion.Maui.Carousel;assembly=Syncfusion.Maui.Carousel"
             Title="ProfileDetailsPage">

    <ContentPage.Resources>
        <ResourceDictionary>
            <toolkit:BoolToObjectConverter x:Key="BoolToObjectConverter-Liked" TrueObject="#008000" FalseObject="#808080" />
            <toolkit:BoolToObjectConverter x:Key="BoolToObjectConverter-Blocked" TrueObject="#FF6347" FalseObject="#808080" />
            <DataTemplate x:Key="itemTemplate">
                <Image Source="{Binding Image}" 
                       Aspect="AspectFit"/>
            </DataTemplate>
        </ResourceDictionary>
    </ContentPage.Resources>

    <ScrollView>
        <ContentView>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition>
                    </ColumnDefinition>
                </Grid.ColumnDefinitions>

                <carousel:SfCarousel
    x:Name="carousel"
                    ItemTemplate="{StaticResource itemTemplate}"
    HeightRequest="400"
    ItemHeight="300"
    ItemWidth="200"
    ItemsSource="{Binding ProfilePhotos}"
    ViewMode="Default"
    WidthRequest="800" />
                 

                <ContentView Grid.Row="1" Grid.Column="0">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="4*" />
                        </Grid.RowDefinitions>

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        
                        <Frame 
                    Grid.Row="1"
Grid.Column="0"
Margin="0,10,0,0"
Padding="16,24,16,0"
BackgroundColor="White"
BorderColor="White"
CornerRadius="20"
HasShadow="true"
HorizontalOptions="Fill">
                    <StackLayout>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label Grid.Row="0" Grid.ColumnSpan="2" Margin="0,0,0,2" FontSize="20" Text="Actions" />

                            <Label IsVisible="{Binding Liked}" Grid.Row="0" x:Name="lblLiked"></Label>
                            <Label IsVisible="{Binding Blocked}" Grid.Row="0" x:Name="lblBlocked"></Label>

                            <ImageButton BackgroundColor="Transparent" Grid.Row="1" CornerRadius="20" Grid.Column="0" 
                                         HeightRequest="20" Command="{Binding LikeCommand}" HorizontalOptions="End">
                                <ImageButton.Source>
                                    <FontImageSource BindingContext="{x:Reference lblLiked}" 
                                                     Color="{Binding IsVisible, Converter={StaticResource BoolToObjectConverter-Liked}}" 
                                                     FontFamily="FontAwesomeSolid" 
                                                     Glyph="{x:Static FoA:FontAwesomeIcons.ThumbsUp}"
                                                     >
                                        
                                    </FontImageSource>
                                </ImageButton.Source>
                            </ImageButton>

                            <ImageButton Grid.Row="1" Grid.Column="1" CornerRadius="20" HeightRequest="20"
                                         Command="{Binding BlockCommand}" HorizontalOptions="Start">
                                <ImageButton.Source>
                                    <FontImageSource BindingContext="{x:Reference lblBlocked}"
                                                     Color="{Binding IsVisible, Converter={StaticResource BoolToObjectConverter-Blocked}}"  
                                                     FontFamily="FontAwesomeSolid" 
                                                     Glyph="{x:Static FoA:FontAwesomeIcons.Ban}">

                                    </FontImageSource>
                                </ImageButton.Source>
                            </ImageButton>

                            <Button Grid.Row="2" Grid.Column="0" TextColor="White" Text="Request Photo" 
          Command="{Binding PhotoRequestCommand}" BackgroundColor="#93c393" 
BorderColor="DarkSeaGreen" BorderWidth="1" Margin="0,10,10,10" />

                            <Button Grid.Row="2" Grid.Column="1" TextColor="White" Text="Send Interest" 
          Command="{Binding SendInterestCommand}" BackgroundColor="#93c393" 
BorderColor="DarkSeaGreen" BorderWidth="1" Margin="0,10,0,10"/>

                            <Button Grid.Row="3" Grid.ColumnSpan="2" TextColor="White" Text="Horoscope Request" 
        Command="{Binding HoroscopeRequestCommand}" BackgroundColor="#93c393" 
BorderColor="DarkSeaGreen" BorderWidth="1" Margin="0,10,0,10" IsVisible="{Binding ShowHinduFields}"/> 

                        </Grid>
                    </StackLayout>

                </Frame>
                        
                        <Frame
    Grid.Row="2"
    Grid.Column="0"
    Margin="0,10,0,0"
    Padding="16,24,16,0"
    BackgroundColor="White"
    BorderColor="White"
    CornerRadius="20"
    HasShadow="true"
    HorizontalOptions="Fill">
                            <StackLayout>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Label Grid.Row="0" Grid.ColumnSpan="2"
        Margin="0,0,0,16"
        FontSize="20"
        Text="Basic Details" />

                                    <Label Grid.Row="1" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="ID" />

                                    <Label Grid.Row="1" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  WebsiteIdentifier}"  />

                                    <Label Grid.Row="2" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="Last Active On" />

                                    <Label Grid.Row="2" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  LastLoggedIn}" />

                                    <Label Grid.Row="3" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Name" />

                                    <Label Grid.Row="3" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  FirstName}" />

                                    <Label Grid.Row="4" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Mother Tongue" />

                                    <Label Grid.Row="4" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  MotherTongue}" />

                                    <Label Grid.Row="5" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "BirthDate" />

                                    <Label Grid.Row="5" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  BirthDate}" />

                                    <Label Grid.Row="6" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "CreatedBy" />

                                    <Label Grid.Row="6" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  ProfileCreatedBy}" />
                                </Grid>
                            </StackLayout>

                        </Frame>

                        <Frame
Grid.Row="3"
Grid.Column="0"
Margin="0,10,0,0"
Padding="16,24,16,0"
BackgroundColor="White"
BorderColor="White"
CornerRadius="20"
HasShadow="true"
HorizontalOptions="Fill">

                            <StackLayout>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Label Grid.Row="0" Grid.ColumnSpan="2"
                                Margin="0,0,0,16"
                                FontSize="20"
                                Text="Religion" />

                                    <Label Grid.Row="1" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="Religion" />

                                    <Label Grid.Row="1" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Religion}" />

                                    <Label Grid.Row="2" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="Caste" />

                                    <Label Grid.Row="2" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Caste}" />

                                    <Label Grid.Row="3" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Denomination" />

                                    <Label Grid.Row="3" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Denomination}" />


                                    <Grid IsVisible="{Binding ShowHinduFields}">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Label Grid.Row="0" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "SubCaste" />

                                        <Label Grid.Row="0" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  SubCaste}" />

                                        <Label Grid.Row="1" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Community" />

                                        <Label Grid.Row="1" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Community}" />

                                        <Label Grid.Row="2" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Birth Time" />

                                        <Label Grid.Row="2" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text ="{Binding BirthTime}" />

                                        <Label Grid.Row="3" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Birth Place" />

                                        <Label Grid.Row="3" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  BirthPlace}" />

                                        <Label Grid.Row="4" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12" Text = "Gothram" />

                                        <Label Grid.Row="4" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Gothram}" />

                                        <Label Grid.Row="5" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Raasi" />

                                        <Label Grid.Row="5" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Raasi}" />

                                        <Label Grid.Row="6" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Star" />

                                        <Label Grid.Row="6" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Star}" />
                                    </Grid>

                                    <Label Grid.Row="7" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Caste Details" />

                                    <Label Grid.Row="7" Grid.Column="1"
        Margin="0,0,0,5" FontSize="14"
        
        Text="{Binding  CasteInformation}" />

                                </Grid>
                            </StackLayout>
                        </Frame>

                        <Frame
Grid.Row="4"
Grid.Column="0"
Margin="0,10,0,0"
Padding="16,24,16,0"
BackgroundColor="White"
BorderColor="White"

CornerRadius="20"
HasShadow="true"
HorizontalOptions="Fill">

                            <StackLayout>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Label Grid.Row="0" Grid.ColumnSpan="2"
        Margin="0,0,0,16"
        FontSize="20"
        Text="About Me" />

                                    <Label Grid.Row="1" Grid.Column="0"
        Margin="0,0,0,5" FontSize="12" FontAttributes="Bold, Italic" Text="Height" />

                                    <Label Grid.Row="1" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Height}" />

                                    <Label Grid.Row="2" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="Weight" />

                                    <Label Grid.Row="2" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Weight}" />

                                    <Label Grid.Row="3" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="BloodGroup" />

                                    <Label Grid.Row="3" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  BloodGroup}" />

                                    <Label Grid.Row="4" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Physical Status" />

                                    <Label Grid.Row="4" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding PhysicalStatus}" />

                                    <Label Grid.Row="5" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Body Type" />

                                    <Label Grid.Row="5" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  BodyType}" />

                                    <Label Grid.Row="6" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Complexion" />

                                    <Label Grid.Row="6" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Complexion}" />

                            <Label Grid.Row="7" Grid.Column="0"
Margin="0,0,0,5" FontAttributes="Bold, Italic" 
FontSize="12"
Text = "About me" />

                            <Label Grid.Row="7" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  AboutMe}" />

                                </Grid>
                            </StackLayout>
                        </Frame>

                        <Frame
Grid.Row="5"
Grid.Column="0"
Margin="0,10,0,0"
Padding="16,24,16,0"
BackgroundColor="White"
BorderColor="White"
CornerRadius="20"
HasShadow="true"
HorizontalOptions="Fill">

                            <StackLayout>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Label
        Margin="0,0,0,16" Grid.Row="0" Grid.ColumnSpan="2"
        FontSize="20"
        Text="Academics/Job" />

                                    <Label Grid.Row="1" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="Education" />

                                    <Label Grid.Row="1" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Education}" />

                                    <Label Grid.Row="2" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="Education Details" />

                                    <Label Grid.Row="2" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  EducationDetails}" />

                                    <Label Grid.Row="3" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Annual Income" />

                                    <Label Grid.Row="3" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  AnnualIncome}" />

                                    <Label Grid.Row="4" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Job Type" />

                                    <Label Grid.Row="4" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding JobType}" />

                                    <Label Grid.Row="5" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Job" />

                                    <Label Grid.Row="5" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  Job}" />

                                    <Label Grid.Row="6" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Employment Details" />

                                    <Label Grid.Row="6" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  EmploymentDetails}" />

                                    <Label Grid.Row="7" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Income Details" />

                                    <Label Grid.Row="7" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  IncomeDetails}" />

                                    <Label Grid.Row="8" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text = "Property Details" />

                                    <Label Grid.Row="8" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  PropertyDetails}" />
                                </Grid>
                            </StackLayout>
                        </Frame>

                        <Frame
Grid.Row="6"
Grid.Column="0"
Margin="0,10,0,0"
Padding="16,24,16,0"
BackgroundColor="White"
BorderColor="White"
CornerRadius="20"
HasShadow="true"
HorizontalOptions="Fill">

                            <StackLayout>

                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Label
        Margin="0,0,0,16" Grid.Row="0" Grid.ColumnSpan="2"
        FontSize="20"
        Text="Family Details" />

                                    <Label Grid.Row="1" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic"  Text="Family Type" />

                                    <Label Grid.Row="1" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  FamilyType}" />

                                    <Label Grid.Row="2" Grid.Column="0"
        Margin="0,0,0,5"
        FontSize="12" FontAttributes="Bold, Italic" 
        Text="Family Value" />

                                    <Label Grid.Row="2" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  FamilyValue}" />

                                    <Label Grid.Row="3" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Family Status" />

                                    <Label Grid.Row="3" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding  FamilyStatus}" />

                                    <Label Grid.Row="4" Grid.Column="0"
        Margin="0,0,0,5" FontAttributes="Bold, Italic" 
        FontSize="12"
        Text="Family Details" />

                                    <Label Grid.Row="4" Grid.Column="1"
        Margin="0,0,0,5"
        FontSize="14"
        
        Text="{Binding FamilyDetails}" />

                                    <Label Grid.Row="5" Grid.Column="0"
