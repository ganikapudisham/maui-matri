                                                        Margin='{OnPlatform Default="6,0,0,0",MacCatalyst="12,0,0,0"}'/>
                                        <Label Text="Mother Tongue " Grid.Row="3" Grid.Column="0" 
                                                        Margin='{OnPlatform Default="6,0,0,0",MacCatalyst="12,0,0,0"}'/>
                                        <Label Text="Birth date" Grid.Row="4" Grid.Column="0" 
                                                        Margin='{OnPlatform Default="6,0,0,0",MacCatalyst="6,0,0,0"}'/>
                                        <Label Text="{Binding WebsiteIdentifier}" Grid.Row="0" Grid.Column="1" 
                                                        Margin='{OnPlatform MacCatalyst="6,0,0,0"}'/>
                                        <Label Text="{Binding LastLoggedIn}" Grid.Row="1" Grid.Column="1" 
                                                        Margin='{OnPlatform MacCatalyst="6,0,0,0"}'/>
